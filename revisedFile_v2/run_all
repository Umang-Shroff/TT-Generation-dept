' Put Module names and Sub names in the order to run the files
Sub RunAllModules()
    ' Call the subroutines from different modules
    Call unmerge.UnmergeAndRetainValuesInFixedRange
    Call create.CreateAndCopyColumnASheetsForClassAndLab
    Call lab.StringMatchingLab22
    Call lec.StringMatchingLec
    Call merge.MergeSameValueCellsInFixedRange
End Sub
